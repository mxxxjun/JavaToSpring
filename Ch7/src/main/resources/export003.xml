<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="koreaTire" class="org.example.ch7.export003.KoreaTire"></bean>
    <bean id="americaTire" class="org.example.ch7.export003.AmericaTire"></bean>
    <bean id="car" class="org.example.ch7.export003.Car">
        <property name="tire" ref="americaTire"></property>
    </bean>
</beans>